class correlation{
    
public:    

    double solve();  
     
     
    double r = 0;
    double sum1 = 0;
    double sum2 = 0; 
    double mean1 = 0; 
    double mean2 = 0;
    double stdev1 = 0;
    double stdev2 = 0;
    double range1 = 0;
    double range2 =0;
    double numerator = 0;
    
    
    double numbers = 63; 
    
    double D1[63] = { 
       
